<div class="container">
<div class="form-group">
  <h3>Sign up to get info on the latest samples sales.</h3>
  <%= render partial: 'form' %>
  <%= form_for @user do |f| %>
    <%= f.label :Username %>
    <%= f.text_field :name %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit 'Create user', :class =>'btn btn-primary' %>
  <% end %>
</div>
</div>
